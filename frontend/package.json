    "axios": "^1.5.1",
    "pinia": "^2.1.6",
    "stylelint-config-recommended-vue": "^1.5.0",
    "swiper": "^10.3.1",
    "vite": "^4.4.9",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.5.1",
    "@testing-library/cypress": "^10.0.1",
