  "private": true,
  "scripts": {
    "preview": "vite preview",
    "build": "vite build",
    "compose:up": "docker compose -f ../docker-compose.yml --profile backend-only up -d",
    "dev": "vite",
    "start": "API_HOST=http://localhost:5000 vite",
    "lint": "eslint . --ignore-path ../.gitignore",
